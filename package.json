{
  "name": "leetcode-practice",
  "version": "1.0.0",
  "description": "A leetcode practice project",
  "main": "index.js",
  "type": "module",
  "imports": {
    "#common/*": "./common/*"
  },
  "bin": {
    "lc": "./bin/lc-enter.js",
    "lk": "./bin/lk-enter.js"
  },
  "scripts": {
    "leet-create": "node common/scripts/create.js",
    "leet-create:i": "node common/scripts/create.js -i",
    "leet-check": "node common/scripts/check.js",
    "commit": "cz",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --ext .js,jsx,cjs,mjs ./common"
  },
  "author": "EternalHeart",
  "license": "ISC",
  "dependencies": {
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "jsdom": "^23.2.0",
    "vite": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "inquirer": "^9.2.14",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5",
    "vitest": "^1.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
